{
    "compilerOptions": {
        "target": "es2015",
        "noImplicitAny": true,
        "module": "commonjs",
        "sourceMap": true,
        "skipLibCheck": true,
        "lib": [
            "es5",
            "esnext",
            "es6",
            "dom",
            "es2015.core",
            "es2015.collection",
            "es2015.generator",
            "es2015.iterable",
            "es2015.promise",
            "es2015.proxy",
            "es2015.reflect",
            "es2015.symbol",
            "es2015.symbol.wellknown",
            "esnext.asynciterable"
        ],
        "experimentalDecorators": true,
        "allowJs": true
    }
}
